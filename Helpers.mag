debug := false;

debugLog := procedure(str)
    if debug then
        str;
    end if;
end procedure;


SNFToOrdersAndBasis := function(snf, basis) 
    dim := #basis;

    orderAndBasis := [];
    for i in [1 .. dim] do
        Append(~orderAndBasis, <snf[i,i], basis[i]>);
    end for;

    return orderAndBasis;
end function;